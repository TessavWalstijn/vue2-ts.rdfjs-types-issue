{
  "name": "vue3-ts.template",
  "version": "1.0.0",
  "description": "Template for Vue3, TS, ESLint + Prettier",
  "repository": "git@github.com:TessavWalstijn/vue3-ts.template.git",
  "author": "TessavWalstijn <email@TessavWalstijn.com>",
  "license": "GPL-3.0",
  "type": "module",
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "dev": "vite serve",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}\"",
    "lint:fix": "yarn lint:format && yarn lint:es-fix",
    "lint:es-fix": "eslint --fix \"src/**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}\"",
    "lint:format": "prettier --ignore-path .gitignore --write \"src/**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}\""
  },
  "dependencies": {
    "@rdfjs/fetch": "^3.1.1",
    "@zazuko/env": "^1.11.1",
    "rdf-ext": "^2.5.1",
    "typescript": "5.1.6",
    "uuid": "^9.0.1",
    "vitest": "^1.6.0",
    "vue": "2.7.16",
    "vue-tsc": "^1.8.27"
  },
  "devDependencies": {
    "@rdfjs/types": "^1.1.0",
    "@types/rdf-ext": "^2",
    "@types/rdfjs__fetch": "^3",
    "@types/uuid": "^9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue2": "^2.3.1",
    "@vitejs/plugin-vue2-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.4.21",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.24.0",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "ts-node": "^10.9.2",
    "unplugin-vue2-script-setup": "^0.11.4",
    "vite": "4.5.3",
    "vite-plugin-vue2": "^2.0.3",
    "vue-template-compiler": "^2.7.16",
    "vue-tsc-eslint-hook": "^1.8.27"
  }
}
